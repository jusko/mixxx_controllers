<?xml version='1.0' encoding='utf-8'?>
<MixxxControllerPreset schemaVersion="1" mixxxVersion="">
    <info>
        <name>Numark DJ2GO2</name>
        <author>jusko</author>
        <description>Very basic controller mappings</description>
    </info>
    <controller id="Numark DJ2GO2">
        <scriptfiles/>
        <controls>
            <!-- Master controls -->
            <control>
                <group>[Master]</group>
                <key>gain</key>
                <description>Master Gain</description>
                <status>0xBF</status>
                <midino>0x0A</midino>
                <options>
                    <soft-takeover/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>headGain</key>
                <description>Master Headphone Gain</description>
                <status>0xBF</status>
                <midino>0x0C</midino>
                <options>
                    <soft-takeover/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>crossfader</key>
                <description>Crossfader</description>
                <status>0xBF</status>
                <midino>0x08</midino>
                <options>
                    <soft-takeover/>
                </options>
            </control>
            <!-- Channel 1 mappings -->
            <control>
                <group>[Channel1]</group>
                <key>pregain</key>
                <description>Gain (Channel 1)</description>
                <status>0xB0</status>
                <midino>0x16</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>pfl</key>
                <description>Headphones (Channel 1)</description>
                <status>0x90</status>
                <midino>0x1B</midino>
                <options>
                    <switch/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>sync_enabled</key>
                <description>Sync (Channel 1)</description>
                <status>0x80</status>
                <midino>0x02</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>sync_enabled</key>
                <description>Sync (Channel 1)</description>
                <status>0x90</status>
                <midino>0x02</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>cue_default</key>
                <description>Cue (Channel 1)</description>
                <status>0x80</status>
                <midino>0x01</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>cue_default</key>
                <description>Cue (Channel 1)</description>
                <status>0x90</status>
                <midino>0x01</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>play</key>
                <description>Play/pause (Channel 1)</description>
                <status>0x90</status>
                <midino>0x00</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>play</key>
                <description>Play/pause (Channel 1)</description>
                <status>0x80</status>
                <midino>0x00</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>rate</key>
                <description>Playback speed (Channel 1)</description>
                <status>0xB0</status>
                <midino>0x09</midino>
                <options>
                    <soft-takeover/>
                    <invert/>
                </options>
            </control>
            <!-- Channel 2 mappings -->
            <control>
                <group>[Channel2]</group>
