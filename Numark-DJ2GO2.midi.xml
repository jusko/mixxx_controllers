<?xml version='1.0' encoding='utf-8'?>
<MixxxControllerPreset schemaVersion="1" mixxxVersion="2.2.0">
    <info>
        <name>Numark DJ2GO2</name>
        <author>jusko</author>
        <description>Very basic controller mappings</description>
    </info>
    <controller id="Numark DJ2GO2">
        <scriptfiles>
            <file filename="lodash.mixxx.js"/>
            <file filename="midi-components-0.0.js"/>
            <file filename="Numark-DJ2GO2.script.js" functionprefix="NumarkDJ2GO2"/>
        </scriptfiles>
        <controls>
            <!-- Master controls -->
            <control>
                <group>[Master]</group>
                <key>NumarkDJ2GO2.master.level.input</key>
                <description>Master Gain</description>
                <status>0xBF</status>
                <midino>0x0A</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>NumarkDJ2GO2.master.cueLevel.input</key>
                <description>Master Headphone Gain</description>
                <status>0xBF</status>
                <midino>0x0C</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>crossfader</key>
                <description>Crossfader</description>
                <status>0xBF</status>
                <midino>0x08</midino>
                <options>
                    <soft-takeover/>
                </options>
            </control>

            <!-- Sync -->
            <control>
                <group>[Channel1]</group>
                <key>NumarkDJ2GO2.leftDeck.syncButton.input</key>
                <description>Sync pressed (Channel 1)</description>
                <status>0x90</status>
                <midino>0x02</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>NumarkDJ2GO2.leftDeck.syncButton.input</key>
                <description>Sync released (Channel 1)</description>
                <status>0x80</status>
                <midino>0x02</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>

            <control>
                <group>[Channel2]</group>
                <key>NumarkDJ2GO2.rightDeck.syncButton.input</key>
                <description>Sync pressed (Channel 2)</description>
                <status>0x91</status>
                <midino>0x02</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Channel2]</group>
                <key>NumarkDJ2GO2.rightDeck.syncButton.input</key>
                <description>Sync released (Channel 2)</description>
                <status>0x81</status>
                <midino>0x02</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>

            <!-- Cue -->
            <control>
                <group>[Channel1]</group>
                <key>NumarkDJ2GO2.leftDeck.cueButton.input</key>
                <description>Cue (Channel 1)</description>
                <status>0x90</status>
                <midino>0x01</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>NumarkDJ2GO2.leftDeck.cueButton.input</key>
                <description>Cue (Channel 1)</description>
                <status>0x80</status>
                <midino>0x01</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>

            <control>
                <group>[Channel2]</group>
                <key>NumarkDJ2GO2.rightDeck.cueButton.input</key>
                <description>Cue (Channel 2)</description>
                <status>0x91</status>
                <midino>0x01</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Channel2]</group>
                <key>NumarkDJ2GO2.rightDeck.cueButton.input</key>
                <description>Cue (Channel 2)</description>
                <status>0x81</status>
                <midino>0x01</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>

            <!-- Play/Pause -->
            <control>
                <group>[Channel1]</group>
                <key>NumarkDJ2GO2.leftDeck.playButton.input</key>
                <description>Play/pause (Channel 1)</description>
                <status>0x90</status>
                <midino>0x00</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>NumarkDJ2GO2.leftDeck.playButton.input</key>
                <description>Play/pause (Channel 1)</description>
                <status>0x80</status>
                <midino>0x00</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>

            <control>
                <group>[Channel2]</group>
                <key>NumarkDJ2GO2.rightDeck.playButton.input</key>
                <description>Play/pause (Channel 2)</description>
                <status>0x91</status>
                <midino>0x00</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Channel2]</group>
                <key>NumarkDJ2GO2.rightDeck.playButton.input</key>
                <description>Play/pause (Channel 2)</description>
                <status>0x81</status>
                <midino>0x00</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>

            <!-- PFL -->
            <control>
                <group>[Channel1]</group>
                <key>NumarkDJ2GO2.headphonesOn</key>
                <description>Headphones On (Channel 1)</description>
                <status>0x90</status>
                <midino>0x1B</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>NumarkDJ2GO2.headphonesOff</key>
                <description>Headphones Off (Channel 1)</description>
                <status>0x80</status>
                <midino>0x1B</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>

            <control>
                <group>[Channel2]</group>
                <key>NumarkDJ2GO2.headphonesOn</key>
                <description>Headphones on (Channel 2)</description>
                <status>0x91</status>
                <midino>0x1B</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Channel2]</group>
                <key>NumarkDJ2GO2.headphonesOff</key>
                <description>Headphones off (Channel 2)</description>
                <status>0x81</status>
                <midino>0x1B</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>

            <!-- Jog Wheels -->
            <control>
                <group>[Channel1]</group>
                <key>NumarkDJ2GO2.leftDeck.jogWheel.input</key>
                <description>Jogwheel search (Deck 1)</description>
                <status>0xB0</status>
                <midino>0x06</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Channel2]</group>
                <key>NumarkDJ2GO2.rightDeck.jogWheel.input</key>
                <description>Jogwheel search (Deck 2)</description>
                <status>0xB1</status>
                <midino>0x06</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>

            <!-- Browse Button -->
            <control>
                <group>[Channel16]</group>
                <key>NumarkDJ2GO2.shiftModeOn</key>
                <description>Enter shift mode</description>
                <status>0x9F</status>
                <midino>0x06</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Channel16]</group>
                <key>NumarkDJ2GO2.shiftModeOff</key>
                <description>Exit shift mode</description>
                <status>0x8F</status>
                <midino>0x06</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>

            <!-- Load Buttons -->
            <control>
                <group>[Channel16]</group>
                <key>NumarkDJ2GO2.enableSuperGain</key>
                <description></description>
                <status>0x9F</status>
                <midino>0x02</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Channel16]</group>
                <key>NumarkDJ2GO2.disableSuperGain</key>
                <description></description>
                <status>0x9F</status>
                <midino>0x03</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>

            <!-- Unscripted -->
            <control>
                <group>[Channel1]</group>
                <key>pregain</key>
                <description>Gain (Channel 1)</description>
                <status>0xB0</status>
                <midino>0x16</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>rate</key>
                <description>Playback speed (Channel 1)</description>
                <status>0xB0</status>
                <midino>0x09</midino>
                <options>
                    <soft-takeover/>
                    <invert/>
                </options>
            </control>

            <control>
                <group>[Channel2]</group>
                <key>pregain</key>
                <description>Gain (Channel 2)</description>
                <status>0xB1</status>
                <midino>0x16</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel2]</group>
                <key>rate</key>
                <description>Playback speed (Channel 2)</description>
                <status>0xB1</status>
                <midino>0x09</midino>
                <options>
                    <invert/>
                </options>
            </control>
        </controls>
        <outputs/>
    </controller>
</MixxxControllerPreset>
